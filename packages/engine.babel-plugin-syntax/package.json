{
  "name": "@c11/engine.babel-plugin-syntax",
  "version": "4.0.0-alpha.101",
  "description": "Compile engine syntax to js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test:simple": "jest --clearCache && NODE_ENV=test jest --config ../../jest.config.js --runTestsByPath ./specs/producer*",
    "test:update": "jest --clearCache && jest --config ../../jest.config.js --runTestsByPath ./specs/* --updateSnapshot",
    "test:coverage": "jest --config ../../jest.config.js --coverageDirectory='packages/engine.babel-plugin-syntax/coverage' --collectCoverage --collectCoverageFrom='[\"packages/engine.babel-plugin-syntax/src/**/*.{ts,tsx,js,jsx}\"]' --runTestsByPath ./specs/*",
    "test": "yarn run test:coverage"
  },
  "author": {
    "name": "Constantin Dumitrescu",
    "email": "dum.constantin@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/types": "^7.17.0",
    "@c11/engine.types": "4.0.0-alpha.101",
    "@types/babel__core": "^7.1.19",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.181",
    "@types/node": "^16.11.26",
    "babel-plugin-tester": "^10.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@c11/engine.utils": "4.0.0-alpha.101",
