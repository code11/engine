    "@svgr/webpack": "^5.5.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@types/react": "^17.0.20",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-rewrite": "^0.2.0",
